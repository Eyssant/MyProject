# function for countingsort sort
def countingsort(MyList):
  n = len(MyList)
  max = 0
  #find largest element in the Array
  for i in MyList:
    if max < i:
      max = i

  #Create a freq array to store number of occurrences of 
  #each unique elements in the given list
  freq = [0 for i in range(0,max+1)] 
  for i in range(0,n):
    freq[MyList[i]] += 1

  #sort the given array using freq list
  j=0
  for i in range(0, max+1): 
    while freq[i]>0:
      MyList[j] = i
      j +=1
      freq[i] -= 1 

# test counting sort code                 
MyList = [1, 10, 23, 50, 4, 2, 2]
print("Original List")
for i in MyList:
  print(i, end=" ")
print("\n")

countingsort(MyList)
print("Sorted List")
for i in MyList:
  print(i, end=" ")
